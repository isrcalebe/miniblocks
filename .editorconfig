root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 80

[*.sln]
indent_style = tab
tab_width = 2

[*.{xml,xaml,csproj,props,targets,DotSettings}]
indent_style = space
indent_size = 2

[*.cs]
indent_style = space
indent_size = 4